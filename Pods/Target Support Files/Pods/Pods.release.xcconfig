GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FLEX" "${PODS_ROOT}/Headers/Public/Objc-Foundation-Extensions" "${PODS_ROOT}/Headers/Public/Objc-iOS-Extensions"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FLEX" -isystem "${PODS_ROOT}/Headers/Public/Objc-Foundation-Extensions" -isystem "${PODS_ROOT}/Headers/Public/Objc-iOS-Extensions"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FLEX" -l"Objc-Foundation-Extensions" -l"Objc-iOS-Extensions" -l"sqlite3" -l"z" -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods