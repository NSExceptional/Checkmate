GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/FLEX.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MirrorKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Objc_Foundation_Extensions.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Objc_iOS_Extensions.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "FLEX" -framework "MirrorKit" -framework "Objc_Foundation_Extensions" -framework "Objc_iOS_Extensions"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods